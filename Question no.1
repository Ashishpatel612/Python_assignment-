# question number 1
def second_largest(number):
    if len(number)<2:
        return "List must contain at least two unique numbers."
    largest=second_largest=float("-inf")
    for  i in number:
        if i>largest:
            second_largest,largest=largest,i
        elif largest>i>second_largest:
            second_largest=i
    return second_largest if second_largest != float('-inf') else "No second largest number found."
my_list = [200, 100, 4, 45, 99,100,99]
result = second_largest(my_list)
print("Second largest number is:", result)     
