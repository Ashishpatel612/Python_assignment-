def count_characters(input_str):
    vowels = 'aeiouAEIOU'
    digits = '0123456789'
    
    vowel_count = 0
    consonant_count = 0
    digit_count = 0
    special_count = 0
    
    for char in input_str:
        if char in vowels:
            vowel_count += 1
        elif char.isalpha():
            consonant_count += 1
        elif char in digits:
            digit_count += 1
        else:
            special_count += 1
    
    print("Vowels:", vowel_count)
    print("Consonants:", consonant_count)
    print("Digits:", digit_count)
    print("Special characters:", special_count)

# Example usage
text = "Hello, World! 123"
count_characters(text)
